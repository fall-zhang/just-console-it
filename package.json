{
  "name": "just-console-it",
  "displayName": "Just Console It",
  "description": "Just Console It",
  "version": "0.1.0",
  "publisher": "FallZhang",
  "type": "module",
  "scripts": {
    "vsce": "vsce",
    "lint": "eslint .",
    "type": "tsc --noEmit",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "author": {
    "name": "Fall Zhang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fall-zhang/just-console-it"
  },
  "icon": "images/star.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Console",
    "Log"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "javascript",
          "js"
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".mjs",
          ".cjs"
        ]
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "typescript",
          "ts"
        ],
        "extensions": [
          ".ts",
          ".tsx",
          ".mts",
          ".cts"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.log",
        "title": "Just Console It"
      }
    ],
    "keybindings": [
      {
        "command": "extension.log",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.90.0",
    "eslint": "^9.8.0",
    "eslint-config-standard-new": "^0.3.0",
    "@eslint/js": "^9.35.0",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}

{
  "name": "just-console-it",
  "displayName": "Just Console It",
  "description": "Just Console It",
  "version": "0.0.3",
  "publisher": "FallZhang",
  "author": {
    "name": "Fall Zhang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fall-zhang/just-console-it"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Console",
    "Log"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onCommand:extension.log"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "javascript",
          "js"
        ],
        "extensions": [
          ".js"
        ]
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "typescript",
          "ts"
        ],
        "extensions": [
          ".ts"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.log",
        "title": "Console.Log JavaScript Selection"
      }
    ],
    "keybindings": [
      {
        "command": "extension.log",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/vscode": "^1.50.0",
    "eslint": "^9.8.0",
    "@types/node": "^18.0.0"
  }
}